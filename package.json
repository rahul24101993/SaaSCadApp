{
    "name": "my-swf-project",
    "version": "1.0.0",
    "description": "An @swf based solution",
    "scripts": {
        "audit": "node node_modules/@swf/tooling/js/audit.js",
        "build": "react-app-rewired build",
        "clean": "rimraf out *.log *.tgz node_modules/.cache",
        "cucumber": "cucumber",
        "eslint": "eslint build src test --ext .js",
        "generate": "node node_modules/@swf/tooling/js/generator",
        "pregulp": "node node_modules/@swf/tooling/js/createCache.js",
        "start": "react-app-rewired start",
        "startClient": "react-app-rewired start",
        "test": "react-app-rewired test --watchAll=false --no-watchman"
    },

    "config-overrides-path": "node_modules/@swf/core/config-overrides.js",
    "dependencies": {
        "@swf/core": "next",
        "afx-gateway": "next",
        "cra-template": "1.1.3",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-icons": "^4.3.1",
        "react-router-dom": "^6.3.0",
        "react-scripts": "5.0.0"
    },
    "browserslist": {
        "production": [
            "Chrome >= 61",
            "Firefox >= 60",
            "Safari >= 11.1",
            "Opera >= 48",
            "Samsung >= 8.2",
            "Android >= 67",
            "ChromeAndroid >= 74",
            "FirefoxAndroid >= 66",
            "Edge >= 79"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "not IE <= 11"
        ]
    },
    "author": "Siemens",
    "license": "MIT"
}
